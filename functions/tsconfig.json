{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ES2022"],
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "..",
    "paths": {
      "@cloudflare/workers-types": ["node_modules/@cloudflare/workers-types"]
    }
  },
  "include": ["./**/*.ts", "env.d.ts"],
  "exclude": ["node_modules"]
}
